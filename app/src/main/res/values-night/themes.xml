<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.App_listas" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/darkGrey</item>
        <item name="colorPrimaryVariant">@color/grey </item>
        <item name="colorOnPrimary">@color/white</item>
        <item name ="colorOnPrimaryContainer">@color/grey</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkWhite</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/pink_200</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimary</item>
        <item name="homeAsUpIndicator">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="backgroundColor">@color/darkGrey</item>
        <item name="android:buttonStyle">@style/Widget.MaterialComponents.Button</item>
    </style>
</resources>